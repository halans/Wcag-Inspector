{
  "name": "wcag-inspector",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npm run dev:frontend",
    "dev:frontend": "vite --config frontend/vite.config.ts",
    "dev:backend": "wrangler dev --config backend/wrangler.toml",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build --config frontend/vite.config.ts",
    "build:backend": "esbuild backend/src/worker.ts --bundle --format=esm --platform=neutral --target=es2022 --outdir=backend/dist --tsconfig=tsconfig.json --external:node:* --external:whatwg-mimetype --external:whatwg-encoding --external:iconv-lite --external:string_decoder --external:css-what",
    "check": "tsc",
    "test": "vitest run",
    "db:push": "drizzle-kit push"
  },
